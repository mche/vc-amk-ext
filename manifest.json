{

  "manifest_version": 2,
  "name": "VK люди+",
  "version": "1.8",
 
  "description": "Леше надо",
  "icons": {
    "48": "icons/48.png"
  },

  "applications": {
    "gecko": {
      "id": "vk-search@vk.com",
      "strict_min_version": "45.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://vk.com/search*", "https://vk.com/friends*", "https://vk.com/people*"],
      "js": ["jquery.js", "main.js"]
    }
  ]

}

